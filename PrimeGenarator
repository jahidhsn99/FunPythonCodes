# Take the input from the user:
smallNumber = int(input("Enter the first number in range: "))
bigNumber = int(input("Enter the last number in range: "))

for number in range(smallNumber,bigNumber +1):
    if number > 1:
        for i in range (2,number):
            if (number % i) == 0:
                break
            else:
                print(number)
